#runtime "hsp3cl"
#cmpopt varinit 1
;#define DictionaryAliasOff
#include "Dictionary.as"

new@Dictionary dict,"double"

repeat 100
	dcSet dict,"K"+cnt,cnt
loop

dcSet dict,"一二三","be"
dcSet dict,"二三一",500
dcSet dict,"三二一",1000
mes "一二三"
mes dcItem(dict,"一二三")
mes "二三一"
mes dcItem(dict,"二三一")
mes "三二一"
mes dcItem(dict,"三二一")
dcSet dict,"三二一",50
mes dcItem(dict,"三二一")

mes "abcdef"
dcSet dict,"abcdef",999
mes dcItem(dict,"abcdef")

mes "\"\""
dcSet dict,"",777
mes dcItem(dict,"")

dcForeach dict
	mes "key: "+dcKeys(dict,cnt)+" value: "+dcValues(dict,cnt)
loop

