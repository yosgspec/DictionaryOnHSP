#runtime "hsp3cl"
#include "Dictionary.as"

new@Dictionary dc,"double"

repeat 100
	dcSet dc,"K"+cnt,cnt
loop

dcSet dc,"一二三","be"
dcSet dc,"二三一",500
dcSet dc,"三二一",1000
mes "一二三"
mes dcItem(dc,"一二三")
mes "二三一"
mes dcItem(dc,"二三一")
mes "三二一"
mes dcItem(dc,"三二一")
dcSet dc,"三二一",50
mes dcItem(dc,"三二一")

mes "abcdef"
dcSet dc,"abcdef",999
mes dcItem(dc,"abcdef")

mes "\"\""
dcSet dc,"",777
mes dcItem(dc,"")

dcForeach dc
	mes "key: "+dcKeys(dc,cnt)+" value: "+dcValues(dc,cnt)
loop

